<%#<h1>ログイン中だよ</h1>

<%= link_to "ログアウト", users_index_path %>

<h1>新規登録します</h1>


<%= form_tag users_create_path, method: :post, data: { turbo: false } do %>
    <%= label_tag :uid, 'ユーザーID：' %>
    <%= text_field_tag :uid %><br>
    <%= label_tag :pass, 'パスワード：' %>
    <%= password_field_tag :pass %><br>
    <%= submit_tag '登録' %><br>
<% end %>